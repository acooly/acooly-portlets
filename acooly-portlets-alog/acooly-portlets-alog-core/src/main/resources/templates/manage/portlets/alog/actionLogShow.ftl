<div class="card-body">
    <dl class="row">
        <dt class="col-sm-3">ID:</dt>
        <dd class="col-sm-9">${actionLog.id}</dd>
        <dt class="col-sm-3">行为Key:</dt>
        <dd class="col-sm-9">${actionLog.actionKey}</dd>
        <dt class="col-sm-3">行为名称:</dt>
        <dd class="col-sm-9">${actionLog.actionName}</dd>
        <dt class="col-sm-3">行为分组:</dt>
        <dd class="col-sm-9">${actionLog.actionGroup}</dd>
        <dt class="col-sm-3">URL:</dt>
        <dd class="col-sm-9">${actionLog.actionUrl}</dd>
        <dt class="col-sm-3">用户:</dt>
        <dd class="col-sm-9">${actionLog.userKey}</dd>
        <dt class="col-sm-3">cookies:</dt>
        <dd class="col-sm-9">${actionLog.cookies}</dd>
        <dt class="col-sm-3">访问时间:</dt>
        <dd class="col-sm-9">${actionLog.actionTime?string('yyyy-MM-dd HH:mm:ss')!}</dd>
        <dt class="col-sm-3">访问数据:</dt>
        <dd class="col-sm-9">${actionLog.data}</dd>
        <dt class="col-sm-3">平台:</dt>
        <dd class="col-sm-9">${actionLog.os.message()!}</dd>
        <dt class="col-sm-3">渠道:</dt>
        <dd class="col-sm-9">${actionLog.channel.message()!}</dd>
        <dt class="col-sm-3">渠道信息:</dt>
        <dd class="col-sm-9">${actionLog.channelInfo}</dd>
        <dt class="col-sm-3">渠道版本:</dt>
        <dd class="col-sm-9">${actionLog.channelVersion}</dd>
        <dt class="col-sm-3">来源:</dt>
        <dd class="col-sm-9">${actionLog.referer}</dd>
        <dt class="col-sm-3">访问IP:</dt>
        <dd class="col-sm-9">${actionLog.userIp}</dd>
        <dt class="col-sm-3">运营商:</dt>
        <dd class="col-sm-9">${actionLog.isp}</dd>
        <dt class="col-sm-3">国家:</dt>
        <dd class="col-sm-9">${actionLog.country}</dd>
        <dt class="col-sm-3">省:</dt>
        <dd class="col-sm-9">${actionLog.region}</dd>
        <dt class="col-sm-3">市:</dt>
        <dd class="col-sm-9">${actionLog.city}</dd>
        <dt class="col-sm-3">经纬度:</dt>
        <dd class="col-sm-9">${actionLog.lntLat}</dd>
    </dl>
</div>
